title = "On reliability estimation based on fuzzy lifetime data",
journal = "Journal of Statistical Planning and Inference",
volume = "139",
number = "5",
pages = "1750 - 1755",
year = "2009",
note = "Special Issue on Degradation, Damage, Fatigue and Accelerated Life Models in Reliability Testing",
issn = "0378-3758",
doi = "https://doi.org/10.1016/j.jspi.2008.05.048",
url = "http://www.sciencedirect.com/science/article/pii/S0378375808002498",
author = "Reinhard Viertl",
keywords = "Bayesian inference, Fuzzy lifetimes, Reliability estimation",
abstract = "Real lifetime data are never precise numbers but more or less non-precise, also called fuzzy. This kind of imprecision is connected with all measurement results of continuous variables, therefore also with time observations. Imprecision is different from errors and variability. Therefore estimation methods for reliability characteristics have to be adapted to the situation of fuzzy lifetimes in order to obtain realistic results."
}
